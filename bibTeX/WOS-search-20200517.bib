Title = {{Arguments for Integrative Management of Protected Areas in the Cities -
   Case Study in Bucharest City}},
Booktitle = {{ECOSMART - ENVIRONMENT AT CROSSROADS: SMART APPROACHES FOR A SUSTAINABLE
   DEVELOPMENT}},
Series = {{Procedia Environmental Sciences}},
Year = {{2016}},
